// Online C compiler to run C program online
#include <stdio.h>
void swap(int,int);

int main() 
{
    int a,b;
    printf("enter value for a and b\n");
    scanf("%d%d",&a,&b);
    printf("\n\nbefore swapping : a=%d and b=%d",a,b);
    swap(a, b);
    return 0;
    
    
}
void swap (int x,int y)
{
    int temp;
    temp=x;
    x=y;
    y=temp;
    printf("\n\n after swaping:a=%d and b=%d",x,y); 
    
    
}

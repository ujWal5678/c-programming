#include <stdio.h>
#include <string.h>
void reversestring(char str[]){
int len=strlen(str);
for(int i=0,j=len-1;i<j;i++,j--){
    char temp=str[i];
    str[i]=str[j];
    str[j]=temp;

}
    
}

    int main(){
        char str[100]="string";
        printf("original string:%s\n",str);
        reversestring(str);
        printf("reversed string : %s\n",str);
        return 0;
    }
